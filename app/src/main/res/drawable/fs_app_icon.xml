<svg width="192" height="192" viewBox="0 0 192 192" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Background gradient -->
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0f9ff;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#e0f2fe;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#bae6fd;stop-opacity:1" />
    </linearGradient>
    
    <!-- Server rack gradient -->
    <linearGradient id="serverGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e5e7eb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d1d5db;stop-opacity:1" />
    </linearGradient>
    
    <!-- File gradient -->
    <linearGradient id="fileGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8fafc;stop-opacity:1" />
    </linearGradient>
    
    <!-- Share arrow gradient -->
    <linearGradient id="arrowGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#34d399;stop-opacity:1" />
    </linearGradient>
    
    <!-- Drop shadow filter -->
    <filter id="dropshadow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="2"/>
      <feOffset dx="1" dy="2" result="offset"/>
      <feComponentTransfer>
        <feFuncA type="linear" slope="0.4"/>
      </feComponentTransfer>
      <feMerge> 
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/> 
      </feMerge>
    </filter>
    
    <!-- Glow effect -->
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="192" height="192" rx="38" ry="38" fill="url(#bgGradient)"/>
  
  <!-- Server rack background -->
  <rect x="32" y="45" width="128" height="102" rx="8" ry="8" fill="url(#serverGradient)" filter="url(#dropshadow)"/>
  
  <!-- Server rack details -->
  <rect x="40" y="53" width="112" height="16" rx="2" fill="#9ca3af"/>
  <rect x="40" y="75" width="112" height="16" rx="2" fill="#9ca3af"/>
  <rect x="40" y="97" width="112" height="16" rx="2" fill="#9ca3af"/>
  <rect x="40" y="119" width="112" height="16" rx="2" fill="#9ca3af"/>
  
  <!-- LED indicators -->
  <circle cx="45" cy="61" r="2" fill="#10b981" filter="url(#glow)"/>
  <circle cx="52" cy="61" r="2" fill="#10b981" filter="url(#glow)"/>
  <circle cx="45" cy="83" r="2" fill="#10b981" filter="url(#glow)"/>
  <circle cx="52" cy="83" r="2" fill="#f59e0b" filter="url(#glow)"/>
  <circle cx="45" cy="105" r="2" fill="#10b981" filter="url(#glow)"/>
  <circle cx="45" cy="127" r="2" fill="#10b981" filter="url(#glow)"/>
  
  <!-- File documents -->
  <g filter="url(#dropshadow)">
    <!-- First file -->
    <path d="M70 35 L70 20 L85 20 L100 35 L100 65 L70 65 Z" fill="url(#fileGradient)"/>
    <path d="M85 20 L85 35 L100 35" fill="none" stroke="#94a3b8" stroke-width="1"/>
    <rect x="75" y="42" width="20" height="2" fill="#64748b"/>
    <rect x="75" y="47" width="15" height="2" fill="#64748b"/>
    <rect x="75" y="52" width="18" height="2" fill="#64748b"/>
    
    <!-- Second file (slightly offset) -->
    <path d="M110 40 L110 25 L125 25 L140 40 L140 70 L110 70 Z" fill="url(#fileGradient)"/>
    <path d="M125 25 L125 40 L140 40" fill="none" stroke="#94a3b8" stroke-width="1"/>
    <rect x="115" y="47" width="20" height="2" fill="#64748b"/>
    <rect x="115" y="52" width="15" height="2" fill="#64748b"/>
    <rect x="115" y="57" width="18" height="2" fill="#64748b"/>
  </g>
  
  <!-- Share/transfer arrows -->
  <g filter="url(#dropshadow)">
    <!-- Curved arrow from files to server -->
    <path d="M105 50 Q120 35 130 55" fill="none" stroke="url(#arrowGradient)" stroke-width="4" stroke-linecap="round"/>
    <path d="M125 52 L130 55 L127 60" fill="url(#arrowGradient)"/>
    
    <!-- Bidirectional arrow -->
    <path d="M50 140 L142 140" fill="none" stroke="url(#arrowGradient)" stroke-width="4" stroke-linecap="round"/>
    <path d="M45 135 L50 140 L45 145" fill="url(#arrowGradient)"/>
    <path d="M147 135 L142 140 L147 145" fill="url(#arrowGradient)"/>
  </g>
  
  <!-- Share File Server text badge -->
  <rect x="15" y="160" width="162" height="25" rx="12" fill="rgba(255, 255, 255, 0.9)" filter="url(#dropshadow)"/>
  <text x="96" y="176" font-family="Arial, sans-serif" font-size="10" font-weight="bold" text-anchor="middle" fill="#1f2937">SHARE FILE SERVER</text>
  
  <!-- Subtle highlight for depth -->
  <ellipse cx="96" cy="50" rx="70" ry="15" fill="rgba(255,255,255,0.1)"/>
</svg>